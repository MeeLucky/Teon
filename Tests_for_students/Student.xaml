<Window x:Class="Tests_for_students.student"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tests_for_students"
        mc:Ignorable="d"
        Title="Teon : student" Height="331.915" Width="410.106">
    <WrapPanel Orientation="Vertical" x:Name="Main" Margin="20" HorizontalAlignment="Center">
        <WrapPanel x:Name="firstPanel" Orientation="Vertical">
            <TextBlock FontSize="20" Margin="0, 0, 20, 0" HorizontalAlignment="Center">Имя Фамилие</TextBlock>
            <TextBox Width="300" x:Name="C_UserName" FontSize="18"></TextBox>
            <WrapPanel Margin="0, 10" Orientation="Vertical" HorizontalAlignment="Center">
                <Button FontSize="18" Margin="0,10" Click="openTest_Click">Открыть тест</Button>
                <StackPanel>
                    <Button FontSize="18" Click="StartTest_Click">Начать тестирование</Button>
                    <TextBlock x:Name="C_testName" HorizontalAlignment="Center" FontSize="18" Margin="10"/>
                </StackPanel>
            </WrapPanel>
        </WrapPanel>
    </WrapPanel>
</Window>
